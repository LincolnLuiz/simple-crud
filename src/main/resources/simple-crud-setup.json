[
    {
        "title": "api-backend",
        "rootPath": "src/main/java/",
        "templates": [
            {
                "filePath": "../resources/templates/entidade.vm",
                "dest": "model",
                "suffixName": ".java"
            },
            {
                "filePath": "../resources/templates/entidadeDTO.vm",
                "dest": "model",
                "suffixName": "DTO.java"
            }
        ]

    },
    {
        "title": "api-frontend",
        "rootPath": "frontend/app/src/",
        "templates": [
            {
                "filePath": "../resources/templates/list.vm",
                "dest": "components/{entity}",
                "suffixName": "{entity.lower}-list.js"
            },
            {
                "filePath": "../resources/templates/entidadeDTO.vm",
                "dest": "model",
                "suffixName": "Mais{entity}DTO.java"
            }
        ]

    }
]